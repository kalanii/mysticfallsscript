 local channel = game.Workspace.Live[Player].Channeling
 channel:Destroy()
end)
Section:NewButton("Loopdelete Acting", "makes it so u can pain and do spells at same time", function()
    local Player = game.Players.LocalPlayer.Name
game:GetService"RunService".RenderStepped:Connect(function()
 local channel = game.Workspace.Live[Player].Acting
 channel:Destroy()
    end)
end)
Section:NewButton("Makes it so u can do stuff in immobilus", "Read title", function()
local Player = game.Players.LocalPlayer.Name
game:GetService"RunService".RenderStepped:Connect(function()
    local frozen = game.Workspace.Live[Player].Frozen
    frozen:Destroy()
    end)

wait()
local Player = game.Players.LocalPlayer.Name
game:GetService"RunService".RenderStepped:Connect(function()
    local freezeweld = game.Workspace.Live[Player].freezeWeld
    freezeweld:Destroy()
    end)
end)
Section:NewButton("Delete username", "Deletes ur user so its harder for mods to track u", function()
local Player = game.Players.LocalPlayer.Name
game:GetService"RunService".RenderStepped:Connect(function()
    local name = game.Workspace.Live[Player].Head.Username
    name:Destroy()
end)
end)
Section:NewTextBox("custom name", "u can be a faction like rashma hestia", function(txt)

-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = (txt)
}

game:GetService("ReplicatedStorage").Events.FilterText:InvokeServer(unpack(args))



-- change rashma astralis to whatever u want and spawn as custom and boom ur name will be that. u have to rejoin each time u set a new one
end)
Section:NewTextBox("auto accept channel", "put in user to auto accept channels", function(txt2)
 while true do
local args = {
    [1] = "Channel",
    [2] = {
        [1] = true,
        [2] = game.Players[(txt2)].Character
    }
}

game:GetService("ReplicatedStorage").Events.Witch:FireServer(unpack(args))
wait(1)
local args = {
    [1] = "Channel",
    [2] = {
        [1] = true,
        [2] = game.Players[(txt2)].Character
    }
}

game:GetService("ReplicatedStorage").Events.Witch:FireServer(unpack(args))
wait(1)
end
    end)
Section:NewTextBox("autosend channels", "put in user to autosend channels", function(txt3)
    while true do 
    local args = {
    [1] = "Magic",
    [2] = {
        [1] = "requestChanneling",
        [2] = game.Players[(txt3)].Character
    }
}

game:GetService("ReplicatedStorage").Events.Witch:FireServer(unpack(args))
wait(1)
    local args = {
    [1] = "Magic",
    [2] = {
        [1] = "requestChanneling",
        [2] = game.Players[(txt3)].Character
    }
}

game:GetService("ReplicatedStorage").Events.Witch:FireServer(unpack(args))
wait(1)
end
end)
